{
  "name": "cyclejs-cookie",
  "version": "0.6.0",
  "description": "Cookie Driver for Cycle.js, based on cookie_js",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register test",
    "browserify": "browserify src/index.js -t babelify -t browserify-shim --exclude rx --exclude cookie_js --outfile dist/cycle-cookie.js",
    "uglify": "uglifyjs dist/cycle-cookie.js -o dist/cycle-cookie.min.js",
    "dist": "npm run browserify && npm run uglify",
    "precompile-lib": "rm -rf lib/ && mkdir -p lib",
    "compile-lib": "babel -d lib/ src/",
    "prepublish": "npm run precompile-lib && npm run compile-lib"
  },
  "browserify-shim": {
    "rx": "global:Rx",
    "cookie_js": "global:cookie"
  },
  "author": "Wiktor ObrÄ™bski <wiktor.obrebski@10clouds.com>",
  "license": "MIT",
  "dependencies": {
    "cookie_js": "^1.1.1",
    "rx": "^4.1.0"
  },
  "devDependencies": {
    "@cycle/core": "^6.0.3",
    "babel-cli": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "browserify-shim": "^3.8.12",
    "istanbul": "^0.4.3",
    "mocha": "^2.4.5",
    "simple-mock": "^0.7.0",
    "uglify": "^0.1.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/10clouds/cyclejs-cookie.git"
  },
  "bugs": "https://github.com/10clouds/cyclejs-cookie/issues"
}
